@prefix icm:  <http://tio.models.tmforum.org/tio/v3.6.0/IntentCommonModel/> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix iexp: <http://intendproject.eu/iexplain#> .

iexp:I5 a icm:Intent ;
    dct:description "Ensure efficient resource cleanup after VM deletion"@en ;
.

iexp:DE5a a icm:DeliveryExpectation ;
    dct:description "Network deallocation time limit"@en ;
    iexp:maxNetworkDeallocationTime "2.0"^^xsd:decimal ;
    iexp:unit "seconds" ;
.

iexp:DE5b a icm:DeliveryExpectation ;
    dct:description "Instance destruction time limit"@en ;
    iexp:maxInstanceDestructionTime "5.0"^^xsd:decimal ;
    iexp:unit "seconds" ;
.
